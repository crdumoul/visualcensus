<%= render :partial => 'ward_charts', :locals => {:category => @category, :ward => @ward2, :which_ward => @which_ward} %>

function doStuff() {
    drawChartsForWard<%= @which_ward %>();
    $("#ward<%= @which_ward %>_name").fadeTo(200, 1);
    $(".ward<%= @which_ward %>_table_div").fadeTo(200, 1, function() {
        $("#loading_dialog").hide();
    });
}
setTimeout(doStuff, 100);
